{
    "version": 2,
    "buildCommand": "npm run build && npm run vercel-build",
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  